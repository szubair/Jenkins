pipeline {
    agent any
    tools { 
        maven '3.6.3' 
    }

    stages {
        stage('TEST'){
		steps {
		  echo "hello-world!"
		  script {
			env.testSkip = input message: 'Would you like Skip this stage?', 
		    	parameters: [string(defaultValue: 'no', description: '', name: 'Please enter yes or no')]
			echo "Response: ${testSkip}"
		  }
    		}
	}
    }
}
