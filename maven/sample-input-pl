pipeline {
    agent any
    tools { 
        maven '3.6.3' 
    }

    stages {
        stage('TEST'){
		steps {
		  echo "hello-world!"
		  script {
			env.testSkip = input message: 'Skip this stage or not?', 
		    	parameters: [string(defaultValue: 'skip', description: '', name: 'response')])
			echo "Response: ${testSkip}"
		  }
    		}
	}
    }
}
