pipeline {
    agent any
    tools { 
        maven '3.6.3' 
    }

    stages {
        stage('TEST'){
		steps {
		  echo "hello-world!"
		  script {
			userInput = input(id: 'Proceed1', message: 'Was this successful?', 
		    	parameters: [[$class: 'BooleanParameterDefinition', defaultValue: true, 
		    	description: '', name: 'response']])
			echo ("Response: "+userInput['response'])
		  }
    		}
	}
    }
}
